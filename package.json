{
  "name": "react-native-modal",
  "version": "11.3.1",
  "description": "An enhanced React-Native modal",
  "main": "src/index.js",
  "types": "typings/index.d.ts",
  "files": [
    "src/",
    "typings/"
  ],
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "react-native run-android --root example/",
