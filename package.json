    "lint:fix": "yarn run lint -- --fix",
    "test": "yarn run lint",
    "release": "yarn semantic-release"
  },
  "keywords": [
    "react-native",
    "react",
    "native",
    "modal",
    "android",
    "ios",
    "backdrop",
    "simple",
    "animated"
